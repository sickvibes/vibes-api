{
  "name": "@sickvibes/vibes-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy": "sls deploy --force",
    "remove": "sls remove",
    "clean": "rm -rf .serverless"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "files": [
      "./packages/*/src/**/*.ts"
    ],
    "printWidth": 120,
    "parser": "typescript"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:prettier/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "ignorePatterns": [
      "**/.serverless/*",
      "**/node_modules/*",
      "**/generated-*",
      "**/*snapshot.spec.ts"
    ],
    "rules": {}
  },
  "dependencies": {
    "aws-sdk": "2.973.0",
    "cross-fetch": "3.1.4",
    "ethers": "5.4.5",
    "ethers-multicall": "0.2.1",
    "lodash": "4.17.21",
    "p-queue": "7.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.64",
    "@types/jest": "26.0.18",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "serverless": "2.55.0",
    "serverless-deployment-bucket": "1.3.0",
    "serverless-webpack": "5.5.1",
    "ts-jest": "26.5.0",
    "ts-loader": "9.2.5",
    "ts-node": "9.1.1",
    "typescript": "4.3.5"
  }
}
